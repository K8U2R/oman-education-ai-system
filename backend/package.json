{
  "name": "oman-education-ai-backend",
  "version": "1.0.0",
  "description": "Oman Education AI System - Backend Services",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "start": "npm run swagger && npm run build:bundle && node dist/index.js",
    "start:dev": "tsx watch src/index.ts",
    "prebuild": "npm run type-check && npm run lint",
    "build": "tsc",
    "build:bundle": "tsup",
    "build:prod": "npm run prebuild && tsup",
    "clean:legacy": "tsx scripts/backend/clean_legacy.ts",
    "audit:security": "npm audit",
    "fix:all": "eslint src --ext .ts --fix",
    "test": "vitest run",
    "test:watch": "vitest --watch",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:backend": "tsx scripts/test-backend-performance.ts",
    "test:api:chat": "vitest run tests/e2e/chat.test.ts",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "type-check": "tsc --noEmit",
    "format": "prettier --write \"src/**/*.{ts,json}\"",
    "format:check": "prettier --check \"src/**/*.{ts,json}\"",
    "validate": "npm run prebuild && npm run format:check && npm run test && npm run build",
    "swagger": "echo 'Generating Swagger Docs...' && tsx scripts/backend/generate_swagger.ts || echo 'Swagger generation skipped (script not found)'",
    "prepare": "husky",
    "scripts:verify-memory": "tsx scripts/backend/verify_memory.ts",
    "scan:styles": "tsx scripts/backend/scan_scss.ts",
    "scripts:verify-rag": "tsx scripts/backend/verify_rag.ts",
    "db:init": "tsx scripts/backend/database/init.ts",
    "db:migrate": "tsx scripts/backend/database/migrate.ts",
    "db:seed": "tsx scripts/backend/database/seed.ts",
    "db:reset": "tsx scripts/backend/database/reset.ts",
    "test:auth-flow": "tsx scripts/test-auth-flow.ts"
  },
  "keywords": [
    "education",
    "ai",
    "backend",
    "typescript",
    "express"
  ],
  "author": "Oman Education AI System",
  "license": "MIT",
  "dependencies": {
    "@aws-sdk/client-ses": "^3.490.0",
    "@prisma/client": "^7.3.0",
    "@sendgrid/mail": "^8.1.0",
    "@supabase/supabase-js": "^2.91.0",
    "@types/bcryptjs": "^2.4.6",
    "axios": "^1.13.2",
    "bcryptjs": "^3.0.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.3",
    "cors": "^2.8.5",
    "docx": "^9.5.1",
    "dotenv": "^16.6.1",
    "exceljs": "^4.4.0",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-session": "^1.18.1",
    "helmet": "^7.1.0",
    "ioredis": "^5.9.2",
    "js-yaml": "^4.1.0",
    "jsonwebtoken": "^9.0.3",
    "multer": "^2.0.2",
    "nodemailer": "^7.0.12",
    "openai": "^6.16.0",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "pdf-parse": "^2.4.5",
    "pg": "^8.17.1",
    "pptxgenjs": "^4.0.1",
    "redis": "^4.6.13",
    "reflect-metadata": "^0.2.2",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "tsyringe": "^4.10.0",
    "uuid": "^13.0.0",
    "winston": "^3.11.0",
    "ws": "^8.19.0",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/express-session": "^1.18.0",
    "@types/js-yaml": "^4.0.9",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/multer": "^2.0.0",
    "@types/node": "^20.10.5",
    "@types/nodemailer": "^7.0.5",
    "@types/passport": "^1.0.16",
    "@types/passport-google-oauth20": "^2.0.16",
    "@types/pdf-parse": "^1.1.5",
    "@types/pg": "^8.16.0",
    "@types/supertest": "^6.0.2",
    "@types/swagger-jsdoc": "^6.0.1",
    "@types/swagger-ui-express": "^4.1.6",
    "@types/uuid": "^10.0.0",
    "@types/ws": "^8.18.1",
    "@typescript-eslint/eslint-plugin": "^8.53.0",
    "@typescript-eslint/parser": "^8.53.0",
    "@vitest/coverage-v8": "^1.6.1",
    "@vitest/ui": "^1.6.1",
    "eslint": "^8.56.0",
    "husky": "^9.1.7",
    "prettier": "^3.1.1",
    "prisma": "^7.3.0",
    "supertest": "^6.1.6",
    "tsconfig-paths": "^4.2.0",
    "tsup": "^8.5.1",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "vite-tsconfig-paths": "^6.0.4",
    "vitest": "^1.6.1"
  }
}
